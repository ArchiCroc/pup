{{#compare template '===' 'imports'}}
import ColorTag from '{{uiPathOffset}}../collection/components/ColorTag';
{{else}}
<ColorTag
  name={ {{~value}}?.name ?  {{~value}}.name : i18n.__('{{ pascalCase @root.pluralName }}.no_color')}
  color={ {{~value}}?.color && `#${ {{~value}}.color}`}
/>
{{/compare}}