{{#if import}}
import { Link } from 'react-router-dom';
{{else}}
{{#*inline "fieldKeyValueIndex" ~}}
  {{~> fieldKey }}.{{#if reference.valueKey}}{{reference.valueKey}}{{else}}{{@root.urlFieldKey}}{{/if ~}}
{{~/inline}}
{{#*inline "fieldUrl" ~}}
  {{~#if reference.baseUrl}}{{reference.baseUrl}}{{else}}/{{@root.uiFolderName}}{{/if ~}}
{{~/inline}}
{ {{~> fieldKey}} && (
  <Link to={`{{> fieldUrl }}/${ {{~> fieldKeyValueIndex~}} }`}>
    { {{~> fieldKeyDataIndex~}} }
  </Link>
)}
{{/if}}
