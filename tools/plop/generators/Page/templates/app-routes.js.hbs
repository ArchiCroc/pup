{{#compare route '===' 'user'}}
                  <AuthenticatedRoute
                    exact
                    path="/{{ moduleName }}/{{ urlSlug }}"
                    component={ {{~ pascalCase moduleName }}{{ pascalCase name ~}} }
                    setAfterLoginPath={setAfterLoginPath}
                    {...props}
                    {...state}
                  />
{{/compare}}
{{#compare route '===' 'admin'}}
                  <AuthorizedRoute
                    exact
                    allowedRoles={['admin']}
                    path="/{{ moduleName }}/{{ urlSlug }}"
                    component={ {{~ pascalCase moduleName }}{{ pascalCase name ~}} }
                    setAfterLoginPath={setAfterLoginPath}
                    {...props}
                    {...state}
                  />
{{/compare}}
{{#compare route '===' 'everyone'}}
                  <PublicRoute 
                    path="/{{ moduleName }}/{{ urlSlug }}" 
                    component={ {{~ pascalCase moduleName }}{{ pascalCase name ~}} }
                    {...props} 
                    {...state} 
                  />
{{/compare}}

