<AuthenticatedRoute
  exact
  path="{{ uiRouteBasePath }}"
  component={ {{~ pascalCase pluralName ~}}Page}
  setAfterLoginPath={setAfterLoginPath}
  {...props}
  {...state}
/>
<AuthenticatedRoute
  exact
  path="{{ uiRouteBasePath }}/new"
  component={New{{ pascalCase singularName ~}}Page}
  setAfterLoginPath={setAfterLoginPath}
  {...props}
  {...state}
/>
<AuthenticatedRoute
  exact
  path="{{ uiRouteBasePath }}/:{{urlFieldKey}}"
  component={View{{ pascalCase singularName ~}}Page}
  setAfterLoginPath={setAfterLoginPath}
  {...props}
  {...state}
/>
<AuthenticatedRoute
  exact
  path="{{ uiRouteBasePath }}/:{{urlFieldKey}}/edit"
  component={Edit{{ pascalCase singularName ~}}Page}
  setAfterLoginPath={setAfterLoginPath}
  {...props}
  {...state}
/>