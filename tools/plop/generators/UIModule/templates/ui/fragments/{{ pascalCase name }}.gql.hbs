fragment {{ pascalCase (singular name) }}Attributes on {{ pascalCase (singular name) }} {
  {{#each schema.fields ~}}
    {{~#compare this.showInTableView '||' this.input}}
      {{#if dataIndex}}
  {{convertDataIndexToGraphqlSubQuery dataIndex}}
      {{else}}
  {{@key}}
      {{/if}}
    {{/compare~}}
  {{~/each}}
}


