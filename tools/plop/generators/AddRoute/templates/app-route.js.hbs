{{#compare routeType '===' 'user'}}
                    <AuthenticatedRoute
                      exact
                      path="{{ uiRouteBasePath }}{{#if urlSlug}}/{{ urlSlug }}{{/if}}"
                      component={ {{~ pascalCase componentName ~}} }
                      setAfterLoginPath={setAfterLoginPath}
                      {...props}
                      {...state}
                    />
{{/compare}}
{{#compare routeType '===' 'admin'}}
                    <AuthorizedRoute
                      exact
                      allowedRoles={['admin']}
                      path="{{ uiRouteBasePath }}{{#if urlSlug}}/{{ urlSlug }}{{/if}}"
                      component={ {{~ pascalCase componentName ~}} }
                      setAfterLoginPath={setAfterLoginPath}
                      {...props}
                      {...state}
                    />
{{/compare}}
{{#compare routeType '===' 'everyone'}}
                    <PublicRoute
                      path="{{ uiRouteBasePath }}{{#if urlSlug}}/{{ urlSlug }}{{/if}}" 
                      component={ {{~ pascalCase componentName ~}} }
                      {...props}
                      {...state}
                    />
{{/compare}}