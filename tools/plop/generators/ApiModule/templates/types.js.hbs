export default `
type {{ pascalCase (singular name) }} {
{{#each schema }}
{{#if this.type}}
  {{@key}}: {{this.type}}
{{/if}}
{{/each}}
}

input {{ pascalCase (singular name) }}Input {
{{#each schema }}
{{#compare this.type '&&' this.input}}
  {{@key}}: {{this.type}}{{#compare this.validate.optional '!=' false '||' this.defaultValue '!=' false}}!{{/compare}}
{{/compare}}
{{/each}}
}
`;

