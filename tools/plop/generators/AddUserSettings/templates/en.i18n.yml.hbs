{{#each schema.fields}}
  {{#compare this.table '||' this.input}}
    {{#compare this.input '!=' 'Hidden'}}
{{snakeCase @key}}: {{#compare this.table 'typeof' 'string'}}{{this.table}}{{else}}{{ titleCase @key}}{{/compare}}
    {{#if this.input}}
{{snakeCase @key}}_label: {{titleCase @key}}
      {{#compare this.type '!=' 'Boolean'  }}
        {{#compare this.input 'in' 'Array|Select|Options|Dropdown'  }}
{{snakeCase @key}}_placeholder: Select {{titleCase @key}}
        {{else}}
{{snakeCase @key}}_placeholder: {{titleCase @key}}
        {{/compare}}
      {{/compare}}
    {{/if}}
    {{/compare}}
  {{/compare}}
{{/each}}