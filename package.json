{
  "name": "pup-plus",
  "version": "2.2.0",
  "description": "Get to the Fun Part Faster",
  "private": true,
  "repository": {
    "url": "git@archicroc:archicroc/pup-plus.git",
    "type": "git"
  },
  "author": "cleverbeagle <business@cleverbeagle.com>",
  "license": "MIT",
  "meteor": {
    "mainModule": {
      "client": "imports/startup/client/index.js",
      "server": "imports/startup/server/index.js"
    },
    "nodeModules": {
      "recompile": {
        "simpl-schema": "legacy",
        "query-string": "legacy",
        "strict-uri-encode": "legacy",
        "split-on-first": "legacy",
        "@ctrl/tinycolor": "legacy"
      }
    }
  },
  "scripts": {
    "dev": "meteor --settings settings-development.json --exclude-archs web.browser.legacy",
    "dev-all": "meteor --settings settings-development.json",
    "staging": "DEPLOY_HOSTNAME=us-east-1.galaxy-deploy.meteor.com meteor deploy pup-staging.cleverbeagle.com --settings settings-staging.json",
    "production": "DEPLOY_HOSTNAME=us-east-1.galaxy-deploy.meteor.com meteor deploy pup.cleverbeagle.com --settings settings-production.json",
    "profile": "meteor --extra-packages bundle-visualizer --production --settings settings-development.json",
    "lint": "meteor npx eslint .",
    "lint:all": "meteor npx eslint --fix .",
    "lint:file": "meteor npx eslint --fix",
    "test": "meteor npx jest",
    "test-watch": "meteor npx jest --watch --verbose false",
    "test-e2e": "meteor npx testcafe chrome:headless imports/ui/**/*.e2e.js",
    "plop": "plop"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@apollo/react-ssr": "^4.0.0",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.14.5",
    "accept-language-parser": "^1.5.0",
    "antd": "^4.16.2",
    "apollo-server": "^2.25.1",
    "apollo-server-express": "^2.25.1",
    "b64-to-blob": "^1.2.19",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "commonmark": "^0.29.3",
    "core-js": "^3.14.0",
    "csv-stringify": "^5.6.2",
    "faker": "^4.1.0",
    "file-saver": "^2.0.5",
    "graphql": "^15.5.0",
    "graphql-scalars": "^1.10.0",
    "graphql-tools": "^6.2.6",
    "handlebars": "^4.7.7",
    "indexof": "0.0.1",
    "isomorphic-fetch": "^3.0.0",
    "jszip": "^3.6.0",
    "juice": "^7.0.0",
    "lodash": "^4.17.21",
    "meteor-node-stubs": "^1.0.3",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "node-fetch": "^2.6.1",
    "polished": "^4.1.3",
    "prop-types": "^15.7.2",
    "query-string": "^6.14.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-markdown": "^5.0.3",
    "react-router": "^5.2.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.0",
    "sanitize-html": "^2.4.0",
    "simpl-schema": "^1.12.0",
    "slugify": "^1.5.3",
    "styled-components": "^5.3.0",
    "uniforms": "^3.5.1",
    "uniforms-antd": "^3.5.1",
    "uniforms-bridge-simple-schema-2": "^3.5.1",
    "url-pattern": "^1.0.3",
    "xml": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/testcafe": "^4.4.0",
    "@testing-library/user-event": "^12.8.3",
    "@types/accept-language-parser": "^1.5.1",
    "@types/file-saver": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/json-schema": "^7.0.7",
    "@types/lodash": "^4.14.170",
    "@types/meteor": "^1.4.70",
    "@types/meteor-universe-i18n": "^1.14.5",
    "@types/node": "^14.17.3",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-scroll": "^1.8.2",
    "@types/simpl-schema": "^1.10.4",
    "@types/styled-components": "^5.1.10",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-lodash": "^3.3.4",
    "enzyme": "^3.11.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-meteor": "^7.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "inquirer-file-selector-prompt": "^1.1.0",
    "inquirer-parse-json-file": "^1.0.2",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^10.5.4",
    "plop": "^2.7.4",
    "pluralize": "^8.0.0",
    "prettier": "^2.3.1",
    "random-generator-cli": "^2.4.4",
    "react-test-renderer": "^17.0.2",
    "testcafe": "^1.14.2",
    "testcafe-react-selectors": "^4.1.5",
    "typescript": "^4.3.2"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json"
    },
    "plugins": [
      "babel",
      "jest",
      "graphql",
      "meteor",
      "prettier",
      "react",
      "testcafe"
    ],
    "extends": [
      "airbnb",
      "prettier",
      "plugin:meteor/recommended",
      "plugin:react/recommended",
      "plugin:testcafe/recommended",
      "plugin:jest/recommended",
      "plugin:prettier/recommended",
      "prettier/@typescript-eslint",
      "plugin:@typescript-eslint/recommended"
    ],
    "env": {
      "browser": true,
      "test": {
        "presets": [
          [
            "env"
          ],
          "react"
        ]
      }
    },
    "globals": {
      "expect": false
    },
    "rules": {
      "arrow-parens": [
        2,
        "always"
      ],
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0,
      "import/extensions": 0,
      "indent": 0,
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "to"
          ]
        }
      ],
      "max-len": 0,
      "no-alert": 0,
      "no-console": 0,
      "no-restricted-globals": 0,
      "no-return-assign": [
        "error",
        "except-parens"
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "__",
            "___",
            "_id",
            "_ids",
            "_str",
            "_ensureIndex",
            "_verifyEmailToken",
            "_resetPasswordToken",
            "_name",
            "_storedLoginToken"
          ]
        }
      ],
      "class-methods-use-this": 0,
      "prettier/prettier": 2,
      "react/jsx-filename-extension": 0,
      "react/jsx-one-expression-per-line": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-props-no-spreading": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-unsafe-assignment": 0,
      "@typescript-eslint/no-unsafe-call": 0,
      "@typescript-eslint/no-unsafe-member-access": 0,
      "@typescript-eslint/restrict-template-expressions": 0,
      "@typescript-eslint/no-floating-promises": 0,
      "@typescript-eslint/ban-ts-comment": 1,
      "@typescript-eslint/no-extra-semi": 0
    },
    "settings": {
      "import/resolver": "meteor",
      "import/ignore": "^Uniforms."
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "./.githooks/pre-commit.sh",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "rangeStart": 0,
    "parser": "babel",
    "proseWrap": "preserve"
  }
}
